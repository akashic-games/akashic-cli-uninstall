    "package.json"
  ],
  "devDependencies": {
    "del": "~3.0.0",
    "gulp": "~3.9.1",
    "gulp-istanbul": "~1.1.3",
    "gulp-jasmine": "~2.3.0",
    "gulp-shell": "~0.5.2",
    "gulp-tslint": "~8.1.3",
    "jasmine": "~2.1.1",
    "jasmine-reporters": "~2.0.4",
    "jasmine-terminal-reporter": "~0.9.1",
    "mdast": "~2.0.0",
